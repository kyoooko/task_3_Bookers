<div class="container main-container">
  <p id="notice"></p>

  <div class="row">
    <!-- 左側 上-->
    <div class="col-xs-3">
      <h2>User info</h2>
      <%= render 'info', user: current_user %>
      <!-- 左側 下-->
      <h2>New book</h2>
      <%= render 'books/create_book_form', book: @book %>
    </div>
    <!-- 右側 -->
    <div class="col-xs-9">
      <h2>Users</h2>
      <%= render 'index',users: @users %>
    </div>  
  </div>
</div>

